{
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "Fira Code",
  "explorer.confirmDelete": false,
  "debug.toolBarLocation": "docked",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Night Owl",
  "workbench.preferredDarkColorTheme": "Night Owl",
  "workbench.preferredLightColorTheme": "Night Owl Light",
  "jsxBracketSameLine": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "tailwindCSS.experimental.classRegex": [["clsx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]],
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "prompt",
  "editor.fontFamily": "Fira Code",
  "editor.fontSize": 13,
  "editor.fontLigatures": true,
  "editor.codeLensFontFamily": "\"\"",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.renderWhitespace": "all",
  "editor.formatOnSave": true,
  "explorer.compactFolders": false,
  "editor.tokenColorCustomizations": {
    "[Night Owl]": {
      "textMateRules": [
        {
          "scope": "entity.name.tag.js",
          "settings": {
            "foreground": "#528abe"
          }
        }
      ]
    }
  },
  "git.enableSmartCommit": true,
  "editor.linkedEditing": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,

  "editor.largeFileOptimizations": false,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 2, ColumnLimit: 0}",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.detectIndentation": false,
  "editor.tabSize": 2,

  "psi-header.lang-config": [
    {
      "language": "python",
      "begin": "",
      "prefix": "# ",
      "end": "",
      "blankLinesAfter": 1
    }
  ],

  "psi-header.templates": [
    {
      "language": "*",
      "template": [
        "----------------------------------------------------------------------------------------------------------------------------------------------",
        "File: <<filename>>",
        "Created Date: <<filecreated('dddd, MMMM Do YYYY, h:mm:ss a')>>",
        "Author: <<author>>",
        "Description: ",
        "----------------------------------------------------------------------------------------------------------------------------------------------"
      ],
      "changeLogCaption": "HISTORY:",
      "changeLogHeaderLineCount": 2,
      "changeLogEntryTemplate": ["<<dateformat('YYYY-MM-DD')>>\t<<initials>>\t"]
    }
  ],
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.formatOnType": true
  },
  "python.formatting.autopep8Path": "",
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["--line-length", "150"],
  "python.languageServer": "Pylance",
  "i18n-ally.defaultNamespace": "common",
  "i18n-ally.dirStructure": "auto",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  "typescript.inlayHints.enumMemberValues.enabled": false,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "languageTool.language": "en",
  "keyboard.touchbar.ignored": [
    "workbench.action.debug.start",
    "workbench.action.debug.run",
    "workbench.action.navigateBack",
    "workbench.action.navigateForward"
  ],
  "nasc-touchbar.addCursorAbove": true,
  "nasc-touchbar.blockComment": true,
  "nasc-touchbar.commentLine": true,
  "nasc-touchbar.showCommands": false,
  "nasc-touchbar.peekDefinition": true,
  "nasc-touchbar.goToDefinition": false,
  "settingsSync.ignoredSettings": ["editor.formatOnSave"],
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.startupEditor": "none",
  "prettier.printWidth": 100,
  "prettier.requireConfig": true,
  "npm.keybindingsChangedWarningShown": true,
  "tailwindCSS.emmetCompletions": true,
  "cmake.configureOnOpen": true,
  "hexeditor.columnWidth": 16,
  "hexeditor.showDecodedText": true,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
  "vscode-color-picker.languages": ["python", "javascript", "typescript", "typescriptreact"],
  "terminal.integrated.defaultProfile.linux": "zsh",
  "ltex.language": "de-AT",
  "ltex.additionalRules.motherTongue": "de-AT",
  "ltex.additionalRules.enablePickyRules": true,
  "ltex.configurationTarget": {
    "dictionary": "workspaceFolder",
    "disabledRules": "workspaceFolder",
    "hiddenFalsePositives": "workspaceFolder"
  },
  "ltex.completionEnabled": true,
  "editor.wordBasedSuggestions": "off",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "editor.inlayHints.fontFamily": "Fira Code",
  "workbench.colorCustomizations": {
    // Name of the theme you are currently using
    "[Night Owl]": {
      "editorInlayHint.foreground": "#f0f0f093",
      "editorInlayHint.background": "#082e4968"
