{
  "git.autofetch": true,
  "git.confirmSync": false,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontFamily": "Roboto Mono for Powerline",
  "explorer.confirmDelete": false,
  "debug.toolBarLocation": "docked",
  "workbench.iconTheme": "material-icon-theme",
  "jsxBracketSameLine": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "tailwindCSS.experimental.classRegex": [],
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  "editor.fontFamily": "'Cascadia Code'",
  "editor.fontSize": 13,
  "editor.fontLigatures": true,
  "editor.codeLensFontFamily": "\"\"",
  "editor.inlineHints.fontFamily": "'Cascadia Code'",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.tokenColorCustomizations": {
    "[Night Owl]": {
      "textMateRules": [
        {
          "scope": "entity.name.tag.js",
          "settings": {
            "foreground": "#528abe"
          }
        }
      ]
    }
  },
  "git.enableSmartCommit": true,
  "editor.linkedEditing": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,

  "editor.largeFileOptimizations": false,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 2, ColumnLimit: 0}",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.detectIndentation": false,
  "editor.tabSize": 2,

  "psi-header.lang-config": [
    {
      "language": "python",
      "begin": "",
      "prefix": "# ",
      "end": "",
      "blankLinesAfter": 1
    }
  ],

  "psi-header.templates": [
    {
      "language": "*",
      "template": [
        "----------------------------------------------------------------------------------------------------------------------------------------------",
        "File: <<filename>>",
        "Created Date: <<filecreated('dddd, MMMM Do YYYY, h:mm:ss a')>>",
        "Author: <<author>>",
        "Description: ",
        "----------------------------------------------------------------------------------------------------------------------------------------------"
      ],
      "changeLogCaption": "HISTORY:",
      "changeLogHeaderLineCount": 2,
      "changeLogEntryTemplate": ["<<dateformat('YYYY-MM-DD')>>\t<<initials>>\t"]
    }
  ],
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.printWidth": 100,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "python.formatting.autopep8Path": "",
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["--line-length", "200"],
  "python.languageServer": "Pylance",
  "i18n-ally.defaultNamespace": "common",
  "i18n-ally.dirStructure": "auto",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "languageTool.language": "en",
  "keyboard.touchbar.ignored": [
    "workbench.action.debug.start",
    "workbench.action.debug.run",
    "workbench.action.navigateBack",
    "workbench.action.navigateForward"
  ],
  "nasc-touchbar.addCursorAbove": true,
  "nasc-touchbar.blockComment": true,
  "nasc-touchbar.commentLine": true,
  "nasc-touchbar.showCommands": false,
  "nasc-touchbar.peekDefinition": true,
  "nasc-touchbar.goToDefinition": false,
  "settingsSync.ignoredSettings": ["editor.formatOnSave"],
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Night Owl"
}
